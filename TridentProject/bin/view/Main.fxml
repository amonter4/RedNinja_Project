<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
   <children>
      <TabPane prefHeight="500.0" prefWidth="700.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <tabs>
          <Tab closable="false" text="Search">
               <content>
                  <fx:include fx:id="searchtab" source="tab/SearchTab.fxml" />
               </content></Tab>
            <Tab closable="false" text="Create Table">
               <content>
                  <fx:include fx:id="createtabletab" source="tab/CreateTableTab.fxml" />
               </content></Tab>
          <Tab closable="false" text="Client">
               <content>
                  <fx:include fx:id="clienttab" source="tab/clientTab.fxml" />
               </content></Tab>
            <Tab closable="false" text="Site">
               <content>
                  <fx:include fx:id="sitetab" source="tab/SiteTab.fxml" />
               </content></Tab>
            <Tab closable="false" text="Shift">
               <content>
                  <fx:include fx:id="shiftTab" source="tab/ShiftTab.fxml" />
               </content></Tab>
            <Tab closable="false" text="Untitled Tab 2" />
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
